        <Label Text="Customer" HorizontalTextAlignment="Center"/>
        <Button Text="Logout" Command="{Binding LogoutCommand}" />
        <Button Text="View Contract" Command="{Binding ViewContractCommand}" />
        <Button Text="View Last Order" Command="{Binding ViewLastOrderCommand}" />
    </VerticalStackLayout>
</ContentPage>
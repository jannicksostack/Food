<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="madtilhjemlose.MVVM.View.User.Admin.CompanyPage"
             >

    <VerticalStackLayout Margin="10" Padding="10" WidthRequest="400" Spacing="10">
        <Image Source="jjmstore.png" HeightRequest="185" Aspect="AspectFit" SemanticProperties.Description="Logo"/>
        <Label Text="Company" VerticalTextAlignment="Center" HorizontalTextAlignment="Center"/>
        <Button Text="Search" Command="{Binding SearchCommand}" />
    </VerticalStackLayout>
</ContentPage>
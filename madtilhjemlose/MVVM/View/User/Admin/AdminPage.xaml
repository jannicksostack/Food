<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="madtilhjemlose.MVVM.View.Admin.AdminPage" >
    <VerticalStackLayout>
        <Label Text="Admin" HorizontalTextAlignment="Center"/>
        <Button Text="Logout" Command="{Binding LogoutCommand}" />
        <Button Text="Contracts" Command="{Binding OpenContractsCommand}" />
        <Button Text="Orders" Command="{Binding OpenOrdersCommand}" />
        <Button Text="Products" Command="{Binding OpenProductsCommand}" />
        <Button Text="Statistics" Command="{Binding OpenStatisticsCommand}" />
    </VerticalStackLayout>
</ContentPage>